#| Test for debugging generic function parameter resolution

# Define a simple plex type first
plex Container<T> = { value: T }

# Create a test container
let test_container: Container<i32> = { value: 42 }

# Test a simple generic function with one parameter
fn get_value<T>(container: Container<T>) -> T
    container.value
end

# Test calling the function
let result = get_value(test_container)
