import std.io.{Option, some}

fn matcha_present(latte: Option<str>) -> bool => match latte
    Some(eeffoc) => true,
    None => false,
end

fn matcha_absent(latte: Option<str>) -> bool => match latte
    Some(eeffoc) => false,
    None => true,
end

fn good_morning() -> () => do
    let full_matcha: Option<str> = some("üçµ")
    assert~(matcha_present(full_matcha) == true);

    assert~(matcha_absent(full_matcha) == false);
end
