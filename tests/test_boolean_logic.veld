# Primitive booleans
let b_true = true
let b_false = false

let bool_eq_true = b_true == true      # should be true
let bool_eq_false = b_false == false   # should be true
let bool_neq = b_true != b_false       # should be true

# Numeric comparisons
let num_eq = 1 == 1                    # true
let num_neq = 1 == 2                   # false
let num_lt = 1 < 2                     # true
let num_gt = 2 > 1                     # true

# String comparisons
let str_eq = "abc" == "abc"            # true
let str_neq = "abc" == "def"           # false

# Record equality
let r1 = { x: 1, y: 2 }
let r2 = { x: 1, y: 2 }
let r3 = { x: 2, y: 2 }
let rec_eq = r1 == r2                  # true
let rec_neq = r1 == r3                 # false

# Array equality
let arr1 = [1, 2, 3]
let arr2 = [1, 2, 3]
let arr3 = [1, 2, 4]
let arr_eq = arr1 == arr2              # true
let arr_neq = arr1 == arr3             # false

# Tuple equality
let tup1 = (1, "a")
let tup2 = (1, "a")
let tup3 = (2, "a")
let tup_eq = tup1 == tup2              # true
let tup_neq = tup1 == tup3             # false

# Enum equality
enum Color
    Red,
    Blue,
end

let c1 = Color.Red
let c2 = Color.Red
let c3 = Color.Blue
let enum_eq = c1 == c2                 # true
let enum_neq = c1 == c3                # false

# Logical operators
let and_true = true and true           # true
let and_false = true and false         # false
let or_true = true or false            # true
let or_false = false or false          # false
let not_true = !true                   # false
let not_false = !false                 # true

# Truthiness checks
let zero_false = 0 == true             # false
let nonzero_true = 1 == true           # false
let empty_str_false = "" == true       # false
let nonempty_str_true = "x" == true    # false

