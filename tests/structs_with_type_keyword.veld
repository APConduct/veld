# Plex definition
type Point = { x: f64, y: f64 } # Structural because of the '{...}'

# Nom def
type PointNom = |{ x: f64, y: f64 } # nominal because of the '|' before the '{...}'

# Struct def
type PointStruct = struct( x: f64, y: f64 ) # nominal and members are private by default

