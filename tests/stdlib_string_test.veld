let a_string = "I'm a string"

let an_upper_string = a_string.to_upper() # Should be "I'M A STRING"

let annotated_string: str = "I'm an Annotated String"

let lower_string = annotated_string.to_lower() # Should be "i'm an annotated string"

let untrimmed = "    Hello, I am an untrimmed string    "

let s_trimmed = untrimmed.trim_start() # "Hello, I am an untrimmed string    "

let t_trimmed = untrimmed.trim() # "Hello, I am an untrimmed string"

let l_trimmed = untrimmed.trim_end() # "    Hello, I am an untrimmed string"




let foo_bar = "Foo Bar"
let bar_foo = "Bar Foo"
let does_contain = foo_bar.contains("o B") # Should be true
let does_not_contain = bar_foo.contains("o B") # Should be false

let does_end_with = foo_bar.ends_with("Bar") # Should be true
let does_not_end_with = foo_bar.ends_with("Foo") # Should be false

let does_start_with = foo_bar.starts_with("Foo") # Should be true
let does_not_start_with = foo_bar.starts_with("Bar") # Should be false

let where_is_your_string = foo_bar.index_of("o ") # Should be 2




let sub_string = foo_bar.substring(0, 3) # Should be "Foo"

let replace_string = foo_bar.replace("Foo", "Bar") # Should be "Bar Bar"

let splitted = foo_bar.split(" ") # Should be ["Foo", "Bar"]

let padded_at_start = foo_bar.pad_start(10, "0") # Should be "00Foo Bar"
let padded_at_end = foo_bar.pad_end(10, "0") # Should be "Foo Bar00"

let repeated = foo_bar.repeat(3) # Should be "Foo BarFoo BarFoo Bar"




