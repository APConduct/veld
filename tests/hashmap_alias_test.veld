# Test HashMap import via collections module alias

import std.collections.HashMap
import std.io

io.println("=== HashMap Alias Import Test ===")
io.println("")

# Test 1: Create HashMap via aliased import
var map = HashMap.new()
io.println("1. Created HashMap via std.collections.HashMap")

# Test 2: Use the HashMap
map.set("key1", "value1")
map.set("key2", "value2")

io.println("2. Added two entries")

# Test 3: Get values
let result1 = map.get("key1")
match result1
    Some(v) => do
        io.print("   key1: ")
        io.println(v)
    end
    None => io.println("   key1 not found")
end

let result2 = map.get("key2")
match result2
    Some(v) => do
        io.print("   key2: ")
        io.println(v)
    end
    None => io.println("   key2 not found")
end

# Test 4: Check length
io.println("")
io.print("3. Map length: ")
io.println(map.len())

io.println("")
io.println("=== Test completed successfully! ===")
