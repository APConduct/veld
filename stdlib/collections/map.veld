#| Map implementation for Veld
#| Provides a key-value mapping data structure

# Map interface kind
pub kind Map<K, V>
    fn get(self, key: K) -> Option<V>
    fn set(self, key: K, value: V) -> ()
    fn has(self, key: K) -> bool
    fn remove(self, key: K) -> bool
    fn clear(self) -> ()
    fn keys(self) -> [K]
    fn values(self) -> [V]
    fn entries(self) -> [(K, V)]
    fn len(self) -> i32
    fn is_empty(self) -> bool

end
