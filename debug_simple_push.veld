# Simplest possible Vec.push test to isolate the type unification bug

fn main()
    std.io.println("=== Simplest Vec.push Test ===")

    # Create a Vec<i32>
    var v: Vec<i32> = std.vec.Vec.new()

    # Try to push a single integer
    v.push(42)

    std.io.println("Push successful!")
end

main()
